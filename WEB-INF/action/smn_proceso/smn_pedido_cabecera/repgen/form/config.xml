<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		Filtro de b�squeda para la generaci�n de reportes
	</summary>
	
	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_entidades_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_cliente_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_documento_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_sucursales_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_area_servicio_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_unidad_servicio_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_almacen_rf.sql' source='sql' scope='transaction'/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_entidades_rf.sql' tag='smn_entidad_rf_rows'/>
		<print mode='table' recordset='query-smn_cliente_id.sql' tag='smn_cliente_id_rows'/>
		<print mode='table' recordset='query-smn_documento_id.sql' tag='smn_documento_id_rows'/>
		<print mode='table' recordset='query-smn_sucursales_rf.sql' tag='smn_sucursal_rf_rows'/>
		<print mode='table' recordset='query-smn_area_servicio_rf.sql' tag='smn_area_servicio_rf_rows'/>
		<print mode='table' recordset='query-smn_unidad_servicio_rf.sql' tag='smn_unidad_servicio_rf_rows'/>
		<print mode='table' recordset='query-smn_almacen_rf.sql' tag='smn_almacen_rf_rows'/>
		
	</output>

</config>
