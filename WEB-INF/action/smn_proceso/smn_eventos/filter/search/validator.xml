<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='eve_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='6'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='eve_codigo' rule='like' />
	<parameter id='eve_descripcion_corta' type='varchar' required='false' label='${lbl:b_description_short}' maxlen='32'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='eve_descripcion_corta' rule='like' />
	<parameter id='smn_prestador_servicio_rf' type='integer' required='false' label='${fld:b_service_provider}' maxlen='10'/>
	<parameter id='eve_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='eve_fecha_registro_ini' date2='eve_fecha_registro_fin' id='eve_fecha_registro_ini' />
	<parameter id='eve_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
