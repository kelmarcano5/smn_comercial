lbl<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncontrolcupo.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_quota_control}"/>
	
	<excel 
		recordset='query-smncontrolcupo.sql' 
		sheetname='${lbl:b_quota_control}' 
		filename='${lbl:b_quota_control}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_evento_id' label='${lbl:b_event}' />
		<col id='ccu_cupo' label='${lbl:b_quota}' />
		<col id='ccu_reservas' label='${lbl:b_booking}' />
		<col id='ccu_saldo' label='${lbl:b_balance}' />
		<col id='ccu_estatus_combo' label='${lbl:b_estatus}' />
		<col id='ccu_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
