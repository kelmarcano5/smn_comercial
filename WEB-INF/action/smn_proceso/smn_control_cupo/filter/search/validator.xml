<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_evento_id' type='integer' required='false' label='${lbl:b_event}' maxlen='10'/>
	<parameter id='ccu_cupo' type='integer' required='false' label='${lbl:b_quota}' maxlen='10'/>
	<parameter id='ccu_reservas' type='integer' required='false' label='${fld:b_booking}' maxlen='10'/>
	<parameter id='ccu_estatus' type='varchar' required='false' label='${lbl:b_estatus}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='ccu_estatus' rule='like' />
	<parameter id='ccu_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='ccu_fecha_registro_ini' date2='ccu_fecha_registro_fin' id='ccu_fecha_registro_ini' />
	<parameter id='ccu_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
