<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnafiliados.sql" source="session" scope="transaction" />
		<recordset id='query-smnauxiliar.sql' source='sql' scope='transaction' />
	
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_afiliate}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_afiliate}">
				<col id='afi_auxiliar_rf' label='${lbl:b_auxiliary}' />
		<col id='afi_fecha_inicio' label='${lbl:b_date_init}' />
		<col id='afi_fecha_final' label='${lbl:b_date_end}' />
		<col id='afi_fecha_desafiliacion' label='${lbl:b_date_desafiliate}' />
		<col id='smn_rol_id' label='${lbl:b_rol}' />
		<col id='smn_servicio_id' label='${lbl:b_services}' />
		<col id='afi_estatus' label='${lbl:b_status}' />
		<col id='afi_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
		<sheet recordset='query-smnauxiliar.sql' sheetname=''>
		<col id='aux_descripcion' label='${lbl:b_description}' />
	</sheet>


	</excel>

</config>
