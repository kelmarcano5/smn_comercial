<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncontrolrenovacion.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_control_renovation}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_control_renovation}">
				<col id='smn_afiliado_id' label='${lbl:b_afiliation}' />
		<col id='smn_pedido_id' label='${lbl:b_order}' />
		<col id='crn_fecha_renovacion' label='${lbl:b_renovation_date}' />
		<col id='crn_fecha_vigencia_hasta' label='${lbl:b_date_vigency_end}' />
		<col id='crn_estatus_combo' label='${lbl:b_estatus}' />
		<col id='crn_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
