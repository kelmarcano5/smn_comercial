<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_afiliate}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_afiliate}' 
		filename='${lbl:b_afiliate}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='aux_descripcion' label='${lbl:b_auxiliary}' />
		<col id='afi_fecha_inicio' label='${lbl:b_date_init}' />
		<col id='afi_fecha_final' label='${lbl:b_date_end}' />
		<col id='smn_rol_id' label='${lbl:b_rol}' />
		<col id='smn_servicio_id' label='${lbl:b_services}' />
		<col id='afi_estatus' label='${lbl:b_status}' />
		<col id='afi_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
