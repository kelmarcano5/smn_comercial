<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='afi_auxiliar_rf' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='afi_fecha_inicio' type='date' required='false' label='${lbl:b_date_init}' maxlen='10'/>
	<parameter id='afi_fecha_final' type='date' required='false' label='${lbl:b_date_end}' maxlen='10'/>
	<parameter id='afi_fecha_desafiliacion' type='date' required='false' label='${lbl:b_date_desafiliate}' maxlen='10'/>
	<parameter id='smn_rol_id' type='integer' required='false' label='${lbl:b_rol}' maxlen='10'/>
	<parameter id='smn_servicio_id' type='integer' required='false' label='${lbl:b_services}' maxlen='10'/>
	<parameter id='afi_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='afi_estatus'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generacion del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
