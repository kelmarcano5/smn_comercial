<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_reservas_id' type='integer' required='false' label='${fld:b_booking_number}' maxlen='10'/>
	<parameter id='smn_evento_id' type='integer' required='false' label='${fld:b_event}' maxlen='10'/>
	<parameter id='smn_afiliado_id' type='integer' required='false' label='${lbl:b_afiliate}' maxlen='10'/>
	<parameter id='smn_relacionado_id' type='integer' required='false' label='${lbl:b_related}' maxlen='10'/>
	<parameter id='res_cantidad' type='integer' required='false' label='${lbl:b_quantity}' maxlen='10'/>
	<parameter id='res_estatus' type='varchar' required='false' label='${lbl:b_estatus}' maxlen='2'/>
	<parameter id='res_fecha_registro' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='res_estatus'
		rule='contains' />

	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generaciï¿½n del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
