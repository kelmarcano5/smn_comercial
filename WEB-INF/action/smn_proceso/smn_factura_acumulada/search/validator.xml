<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_documento_id' type='integer' required='true' label='${lbl:b_document}' maxlen='10'/>
	<parameter id='smn_entidades_rf' type='integer' required='true' label='${lbl:b_entities}' maxlen='10'/>
	<parameter id='smn_sucursales_rf' type='integer' required='true' label='${lbl:b_subsidiaries}' maxlen='10'/>
 	<parameter id='smn_cliente_id' type='integer' required='true' label='${lbl:b_customer}' maxlen='10'/>
 	<!-- <parameter id='smn_tasa_rf' type='integer' required='false' label='${lbl:b_customer}' maxlen='10'/> -->
	<parameter id='pca_fecha_registro_ini' type='date' required='true' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='pca_fecha_registro_ini' date2='pca_fecha_registro_fin' id='pca_fecha_registro_ini' />
	<parameter id='pca_fecha_registro_fin' type='date' required='true' label='${lbl:b_store_date}' maxlen='10'/>
    <parameter id='smn_centro_facturacion_id' type='integer' required='true' label='Centro Facturacion' maxlen='10'/>
</validator>
