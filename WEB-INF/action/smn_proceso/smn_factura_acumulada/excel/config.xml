<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnfacturacion_acumulada.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_header_order}"/>
	
	<excel 
		recordset='query-smnfacturacion_acumulada.sql' 
		sheetname='Facturacion Acumulada' 
		filename='Facturacion Acumulada.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='cliente' label='${lbl:b_customer}}' />
		<col id='monto_ml' label='Monto ML' />
		<col id='monto_ma' label='Monto MA' />

	</excel>

</config>
