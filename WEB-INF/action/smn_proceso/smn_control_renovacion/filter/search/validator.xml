<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_afiliado_id' type='integer' required='false' label='${lbl:b_afiliation}' maxlen='10'/>
	<parameter id='smn_pedido_id' type='integer' required='false' label='${lbl:b_order}' maxlen='10'/>
	<parameter id='crn_fecha_renovacion' type='date' required='false' label='${lbl:b_renovation_date}' maxlen='10'/>
	<parameter id='crn_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='crn_fecha_registro_ini' date2='crn_fecha_registro_fin' id='crn_fecha_registro_ini' />
	<parameter id='crn_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
