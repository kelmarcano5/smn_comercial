<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_afiliado_id' type='integer' required='false' label='${lbl:b_afiliation}' maxlen='10'/>
	<parameter id='smn_pedido_id' type='integer' required='false' label='${lbl:b_order}' maxlen='10'/>
	<parameter id='crn_fecha_renovacion' type='date' required='false' label='${lbl:b_renovation_date}' maxlen='10'/>
	<parameter id='crn_fecha_vigencia_hasta' type='date' required='false' label='${lbl:b_date_vigency_end}' maxlen='10'/>
	<parameter id='crn_estatus' type='varchar' required='false' label='${lbl:b_estatus}' maxlen='2'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='crn_estatus'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
