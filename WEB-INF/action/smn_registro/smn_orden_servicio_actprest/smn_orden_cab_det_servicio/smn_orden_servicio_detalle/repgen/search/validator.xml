<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_orden_cab_det_servicio_id' type='integer' required='false' label='${lbl:b_service_order}' maxlen='10'/>
	<parameter id='smn_servicios_rf' type='integer' required='false' label='${lbl:b_services}' maxlen='10'/>
	<parameter id='smn_item_rf' type='integer' required='false' label='${lbl:b_item}' maxlen='10'/>
	<parameter id='smn_componentes_rf' type='integer' required='false' label='${lbl:b_component}' maxlen='10'/>
	<parameter id='osd_cantidad' type='double' required='false' label='${lbl:b_quantity}' maxlen='10'/>
	<parameter id='smn_tipo_componente' type='varchar' required='false' label='${lbl:b_component_type}' maxlen='2'/>
	<parameter id='smn_grupo_prestador_rf' type='integer' required='false' label='${lbl:b_group_of_provider}' maxlen='10'/>
	<parameter id='smn_prestador_servicio_rf' type='integer' required='false' label='${fld:b_service_provider}' maxlen='10'/>
	<parameter id='smn_nivel_estructura_rf' type='integer' required='false' label='${lbl:b_structure_level}' maxlen='10'/>
	<parameter id='osd_estatus' type='varchar' required='false' label='${lbl:b_estatus}' maxlen='2'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_tipo_componente'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='osd_estatus'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
