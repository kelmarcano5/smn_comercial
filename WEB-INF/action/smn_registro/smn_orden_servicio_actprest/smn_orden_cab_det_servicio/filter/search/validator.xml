<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_orden_servicio_id_name' type='varchar' required='false' label='${lbl:b_service_order}'/>
	<parameter id='smn_orden_servicio_id' type='integer' required='false' label='${lbl:b_service_order}' maxlen='10'/>
	<parameter id='ocd_estatus' type='varchar' required='false' label='${lbl:b_estatus}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='ocd_estatus' rule='like' />
	<parameter id='ocd_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='ocd_fecha_registro_ini' date2='ocd_fecha_registro_fin' id='ocd_fecha_registro_ini' />
	<parameter id='ocd_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
