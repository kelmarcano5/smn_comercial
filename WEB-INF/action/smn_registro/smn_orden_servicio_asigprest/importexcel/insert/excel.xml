<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_documento_id' type='integer' required='true' maxlen='10'/>
	<parameter id='ors_numero' type='integer' required='true' maxlen='10'/>
	<parameter id='smn_cliente_id' type='integer' required='true' maxlen='10'/>
	<parameter id='ors_descripcion' type='varchar' required='true' maxlen='100'/>
	<parameter id='ors_fecha_inicial' type='date' required='false' maxlen='10'/>
	<parameter id='ors_fecha_final' type='date' required='false' maxlen='10'/>
	<parameter id='ors_hora_inicial' type='varchar' required='false' maxlen='8'/>
	<parameter id='ors_hora_final' type='varchar' required='false' maxlen='8'/>
	<parameter id='smn_prestador_servicio_rf' type='integer' required='false' maxlen='10'/>
	<parameter id='ors_observacion' type='varchar' required='false' maxlen='200'/>
	<parameter id='ors_estatus' type='varchar' required='true' maxlen='2'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='ors_descripcion'/>


</validator>
