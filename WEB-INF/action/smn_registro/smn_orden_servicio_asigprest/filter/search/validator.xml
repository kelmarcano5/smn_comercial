<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_documento_id' type='integer' required='false' label='${lbl:b_document}' maxlen='10'/>
	<parameter id='ors_numero' type='integer' required='false' label='${lbl:b_number}' maxlen='10'/>
	<parameter id='smn_cliente_id' type='integer' required='false' label='${lbl:b_customer}' maxlen='10'/>
	<parameter id='ors_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='ors_descripcion' rule='like' />
	<parameter id='smn_prestador_servicio_rf' type='integer' required='false' label='${fld:b_service_provider}' maxlen='10'/>
	<parameter id='ors_estatus' type='varchar' required='false' label='${lbl:b_estatus}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='ors_estatus' rule='like' />
	<parameter id='ors_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='ors_fecha_registro_ini' date2='ors_fecha_registro_fin' id='ors_fecha_registro_ini' />
	<parameter id='ors_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
