<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnordenserviciodetalle.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="5" cellStep="0" name="${lbl:b_service_order_det}"/>
	
	<excel 
		recordset='query-smnordenserviciodetalle.sql' 
		sheetname='${lbl:b_service_order_det}' 
		filename='${lbl:b_service_order_det}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_orden_cab_det_servicio_id' label='${lbl:b_service_order}' />
		<col id='smn_servicios_rf' label='${lbl:b_services}' />
		<col id='smn_item_rf' label='${lbl:b_item}' />
		<col id='smn_componentes_rf' label='${lbl:b_component}' />
		<col id='osd_cantidad' label='${lbl:b_quantity}' />
		<col id='smn_tipo_componente' label='${lbl:b_component_type}' />
		<col id='smn_grupo_prestador_rf' label='${lbl:b_group_of_provider}' />
		<col id='smn_prestador_servicio_rf' label='${fld:b_service_provider}' />
		<col id='smn_nivel_estructura_rf' label='${lbl:b_structure_level}' />
		<col id='osd_estatus' label='${lbl:b_estatus}' />
		<col id='osd_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
