<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnordencabdetservicio.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="2" cellStep="0" name="${lbl:b_header_detail_order_service}"/>
	
	<excel 
		recordset='query-smnordencabdetservicio.sql' 
		sheetname='${lbl:b_header_detail_order_service}' 
		filename='${lbl:b_header_detail_order_service}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='ors_descripcion_pl0' label='${lbl:b_service_order}' />
		<col id='ocd_observacion' label='${lbl:b_observations}' />
		<col id='ocd_estatus' label='${lbl:b_estatus}' />
		<col id='ocd_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
