<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_documento_id' type='integer' required='false' label='${lbl:b_document}' maxlen='10'/>
	<parameter id='fca_numero_documento' type='integer' required='false' label='${lbl:b_document_number}' maxlen='10'/>
	<parameter id='fca_rif' type='varchar' required='false' label='${lbl:b_rif}' maxlen='50'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='fca_rif' rule='like' />
	<parameter id='smn_cliente_id' type='integer' required='false' label='${lbl:b_customer}' maxlen='10'/>
	<parameter id='fca_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='fca_fecha_registro_ini' date2='fca_fecha_registro_fin' id='fca_fecha_registro_ini' />
	<parameter id='fca_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
