<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnfacturabonificacion.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_bill_bonus}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_bill_bonus}">
				<col id='fde_naturaleza_pl0' label='${lbl:b_detail_bill}' />
		<col id='smn_item_bonificar_rf' label='${lbl:b_item_meliorate}' />
		<col id='fbo_cantidad_origen' label='${lbl:b_quantity_origin}' />
		<col id='fbo_cantidad_bonificar' label='${lbl:b_quantity_meliorate}' />
		<col id='fbo_precio_original' label='${lbl:b_price_original}' />
		<col id='fbo_monto' label='${lbl:b_amount}' />
		<col id='fbo_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
