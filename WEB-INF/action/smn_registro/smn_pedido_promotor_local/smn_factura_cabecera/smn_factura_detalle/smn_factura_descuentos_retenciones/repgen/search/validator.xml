<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_factura_detalle_id' type='integer' required='false' label='${lbl:b_detail_bill}' maxlen='10'/>
	<parameter id='smn_descuentos_retenciones_rf' type='integer' required='false' label='${lbl:b_discount_retention}' maxlen='10'/>
	<parameter id='fdr_monto_base_ml' type='double' required='false' label='${lbl:b_amount_base_ml}' maxlen='10'/>
	<parameter id='fdr_porcentaje_descuento' type='double' required='false' label='${lbl:b_percentage_discount}' maxlen='10'/>
	<parameter id='fdr_monto_descuento_ml' type='double' required='false' label='${lbl:b_amount_discount}' maxlen='10'/>
	<parameter id='smn_moneda_rf' type='integer' required='false' label='${lbl:b_coins}' maxlen='10'/>
	<parameter id='smn_tasa_rf' type='integer' required='false' label='${lbl:b_exchange_rate}' maxlen='10'/>
	<parameter id='fdr_monto_descuento_ma' type='double' required='false' label='${lbl:b_amount_discount_ma}' maxlen='10'/>



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
