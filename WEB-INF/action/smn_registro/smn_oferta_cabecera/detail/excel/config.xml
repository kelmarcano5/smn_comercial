<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_offer}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_offer}' 
		filename='${lbl:b_offer}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_documento_id' label='${lbl:b_document}' />
		<col id='ofe_numero_documento' label='${lbl:b_document_number}' />
		<col id='smn_entidad_rf' label='${lbl:b_entities}' />
		<col id='smn_sucursal_rf' label='${lbl:b_subsidiaries}' />
		<col id='ofe_fecha_desde' label='${lbl:b_init_date}' />
		<col id='ofe_fecha_hasta' label='${lbl:b_end_date}' />
		<col id='ofe_observaciones' label='${lbl:b_observations}' />
		<col id='ofe_estatus' label='${lbl:b_status}' />
		<col id='ofe_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
