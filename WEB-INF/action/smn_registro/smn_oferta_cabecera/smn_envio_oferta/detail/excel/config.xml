<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_offer_delivery}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_offer_delivery}' 
		filename='${lbl:b_offer_delivery}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_oferta_id' label='${lbl:b_offer}' />
		<col id='eof_fecha_envio' label='${lbl:b_delivery_date}' />
		<col id='eof_fecha_cierre' label='${lbl:b_close_date}' />
		<col id='smn_clasificacion_cliente_id' label='${lbl:b_customer_clasification}' />
		<col id='smn_clientes_area_interes_id' label='${lbl:b_customer_interesting_area}' />
		<col id='smn_tipo_contactos_rf' label='${lbl:b_type_contacts}' />
		<col id='eof_descripcion' label='${lbl:b_description}' />
		<col id='eof_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
