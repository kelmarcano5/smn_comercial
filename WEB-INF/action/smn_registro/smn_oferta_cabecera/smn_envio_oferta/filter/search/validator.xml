<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_oferta_id' type='integer' required='false' label='${lbl:b_offer}' maxlen='10'/>
	<parameter id='smn_clasificacion_cliente_id' type='integer' required='false' label='${lbl:b_customer_clasification}' maxlen='10'/>
	<parameter id='smn_clientes_area_interes_id' type='integer' required='false' label='${lbl:b_customer_interesting_area}' maxlen='10'/>
	<parameter id='eof_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='200'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='eof_descripcion' rule='like' />
	<parameter id='eof_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='eof_fecha_registro_ini' date2='eof_fecha_registro_fin' id='eof_fecha_registro_ini' />
	<parameter id='eof_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
