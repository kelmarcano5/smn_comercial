<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_oferta_id' type='integer' required='true' maxlen='10'/>
	<parameter id='eof_fecha_envio' type='date' required='false' maxlen='10'/>
	<parameter id='eof_fecha_cierre' type='date' required='false' maxlen='10'/>
	<parameter id='smn_clasificacion_cliente_id' type='integer' required='false' maxlen='10'/>
	<parameter id='smn_clientes_area_interes_id' type='integer' required='false' maxlen='10'/>
	<parameter id='smn_tipo_contactos_rf' type='integer' required='true' maxlen='10'/>
	<parameter id='eof_descripcion' type='varchar' required='true' maxlen='200'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='eof_descripcion'/>


</validator>
