<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnofertadetalle.sql" source="session" scope="transaction" />
		<recordset id='query-smnmonedas.sql' source='sql' scope='transaction' />
	
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_offer_detail}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_offer_detail}">
				<col id='smn_oferta_id' label='${lbl:b_offer}' />
		<col id='ofd_naturaleza' label='${lbl:b_nature}' />
		<col id='smn_servicios_rf' label='${lbl:b_services}' />
		<col id='smn_item_rf' label='${lbl:b_item}' />
		<col id='smn_activo_fijo_rf' label='${lbl:b_actfijo}' />
		<col id='ofd_cantidad' label='${lbl:b_quantity}' />
		<col id='ofd_precio' label='${lbl:b_price}' />
		<col id='ofd_monto_ml' label='${lbl:b_amount}' />
		<col id='ofd_moneda_rf' label='${lbl:b_coins}' />
		<col id='smn_tasa_rf' label='${lbl:b_exchange_rate}' />
		<col id='ofd_precio_moneda_alterna' label='${lbl:b_price_coins_alter}' />
		<col id='ofd_monto_moneda_alterna' label='${lbl:b_amount_coins_alter}' />
		<col id='ofd_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
		<sheet recordset='query-smnmonedas.sql' sheetname=''>
		<col id='mon_nombre' label='${lbl:b_name}' />
	</sheet>


	</excel>

</config>
