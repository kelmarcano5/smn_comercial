<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnpagopuntoventa.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_order_health}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_order_health}">
				<col id='smn_pedido_cabecera_id' label='${lbl:b_header_order}' />
		<col id='smn_mov_caja_cabecera_id' label='${lbl:b_movement_caj}' />
		<col id='smn_pagador_clase_rf' label='${lbl:b_class_auxiliary}' />
		<col id='smn_rel_punto_venta_pago_rf' label='${lbl:b_rel_point_of_sale_order}' />
		<col id='smn_forma_pago_rf' label='${lbl:b_way_to_pay}' />
		<col id='ppv_numero_documento_forma_pago' label='${lbl:b_number_doc_form_pay}' />
		<col id='ppv_numero_autorizacion' label='${lbl:b_number_autorization}' />
		<col id='ppv_documento_identidad_pagador' label='${lbl:b_document_identity}' />
		<col id='ppv_nombre_pagador' label='${lbl:b_paying_name}' />
		<col id='ppv_monto_pagar_ml' label='${lbl:b_amount_payment_ml}' />
		<col id='ppv_monto_pago_ml' label='${lbl:b_amount_pay_ml}' />
		<col id='ppv_monto_devuelto_ml' label='${lbl:b_amount_revers_ml}' />
		<col id='ppv_monto_pagar_ma' label='${lbl:b_amount_payment_ma}' />
		<col id='ppv_monto_pago_ma' label='${lbl:b_amount_payment_ma}' />
		<col id='ppv_monto_devuelto_ma' label='${lbl:b_amount_revers_ma}' />
		<col id='smn_moneda_rf' label='${lbl:b_coins}' />
		<col id='smn_tasa_rf' label='${lbl:b_exchange_rate}' />
		<col id='ppv_observacion' label='${lbl:b_observation}' />
		<col id='ppv_estatus' label='${lbl:b_status}' />
		<col id='ppv_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
