<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="5" cellStep="0" name="${lbl:b_relacion_cliente_entidad_financiera}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_relacion_cliente_entidad_financiera}' 
		filename='${lbl:b_relacion_cliente_entidad_financiera}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_cliente_id' label='${lbl:b_customer}' />
		<col id='smn_entidad_financiera_rf' label='${lbl:b_financial_entities}' />
		<col id='smn_tipo_cuenta_banco_rf' label='${lbl:b_types_of_bank_accounts}' />
		<col id='cef_numero_cuenta' label='${lbl:b_bank_account_number}' />
		<col id='cef_estatus' label='${lbl:b_status}' />
		<col id='cef_vigencia' label='${lbl:b_validation_date}' />
		<col id='cef_idioma' label='${lbl:s_profile_lang}' />
		<col id='cef_usuario' label='${lbl:b_users}' />
		<col id='cef_fecha_registro' label='${lbl:b_store_date}' />
		<col id='cef_hora' label='${lbl:s_loginhist_col2}' />

	</excel>

</config>
