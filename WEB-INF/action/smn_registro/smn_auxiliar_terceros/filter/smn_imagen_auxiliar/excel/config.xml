<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnimagenauxiliar.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnimagenauxiliar.sql' sheetname='${lbl:b_image_customer}' filename='smn_imagen_auxiliar.xls'>
		<col id='smn_auxiliar_rf_combo' label='${lbl:b_auxiliary}' />
		<col id='cca_descripcion' label='${lbl:b_description}' />
		<col id='cca_imagen_size' label='${lbl:b_image_size}' />
		<col id='filename' label='${lbl:b_name}' />
		<col id='content_type' label='${lbl:b_type_content}' />
		<col id='cca_idioma' label='${lbl:s_profile_lang}' />
		<col id='cca_usuario' label='${lbl:b_users}' />
		<col id='cca_fecha_registro' label='${lbl:b_store_date}' />
		<col id='cca_hora' label='${lbl:s_loginhist_col2}' />
	</excel>

</config>