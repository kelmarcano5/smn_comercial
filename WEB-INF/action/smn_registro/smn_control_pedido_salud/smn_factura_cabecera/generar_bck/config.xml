<?xml version='1.0' encoding='ISO-8859-1' ?>

<config>
	
	<sumary> Generar factura </sumary>
	
	<log> false </log>
	
	<transaction>
		<classname>proceso.Generar_factura</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>true</jdbc-log>
