<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_components_order}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_ingresos_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_grupo_componente_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_componente_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_clase_auxiliar_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_auxiliar_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_item_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_servicio_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_moneda_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_tasa_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_grupo_titulo_impresion_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_tipo_gasto_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_sub_tipo_gasto_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_prestador_servicio_rf.sql' source='sql' scope='transaction'/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_ingresos_rf.sql' tag='smn_ingresos_rf_rows'/>
		<print mode='table' recordset='query-smn_grupo_componente_rf.sql' tag='smn_grupo_componente_rf_rows'/>
		<print mode='table' recordset='query-smn_componente_rf.sql' tag='smn_componente_rf_rows'/>
		<print mode='table' recordset='query-smn_clase_auxiliar_rf.sql' tag='smn_clase_auxiliar_rf_rows'/>
		<print mode='table' recordset='query-smn_auxiliar_rf.sql' tag='smn_auxiliar_rf_rows'/>
		<print mode='table' recordset='query-smn_item_rf.sql' tag='smn_item_rf_rows'/>
		<print mode='table' recordset='query-smn_servicio_rf.sql' tag='smn_servicio_rf_rows'/>
		<print mode='table' recordset='query-smn_moneda_rf.sql' tag='smn_moneda_rf_rows'/>
		<print mode='table' recordset='query-smn_tasa_rf.sql' tag='smn_tasa_rf_rows'/>
		<print mode='table' recordset='query-smn_grupo_titulo_impresion_rf.sql' tag='smn_grupo_titulo_impresion_rf_rows'/>
		<print mode='table' recordset='query-smn_tipo_gasto_rf.sql' tag='smn_tipo_gasto_rf_rows'/>
		<print mode='table' recordset='query-smn_sub_tipo_gasto_rf.sql' tag='smn_sub_tipo_gasto_rf_rows'/>
		<print mode='table' recordset='query-smn_prestador_servicio_rf.sql' tag='smn_prestador_servicio_rf_rows'/>
		<print mode="form" recordset="_request" /> 
	</output>

</config>
