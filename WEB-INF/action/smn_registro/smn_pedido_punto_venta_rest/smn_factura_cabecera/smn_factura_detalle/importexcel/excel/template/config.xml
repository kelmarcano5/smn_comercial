<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnfacturadetalle.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_detail_bill}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_detail_bill}">
				<col id='smn_factura_cabecera_id' label='${lbl:b_header_bill}' />
		<col id='fde_naturaleza' label='${lbl:b_nature}' />
		<col id='smn_servicios_rf' label='${lbl:b_services}' />
		<col id='smn_item_rf' label='${lbl:b_item}' />
		<col id='smn_activo_fijo_rf' label='${lbl:b_actfijo}' />
		<col id='fde_cantidad' label='${lbl:b_quantity}' />
		<col id='fde_precio_ml' label='${lbl:b_price}' />
		<col id='fde_monto_ml' label='${lbl:b_amount}' />
		<col id='smn_moneda_rf' label='${lbl:b_coins}' />
		<col id='smn_tasa_rf' label='${lbl:b_exchange_rate}' />
		<col id='fde_precio_ma' label='${lbl:b_price_coins_alter}' />
		<col id='fde_monto_ma' label='${lbl:b_amount_coins_alter}' />
		<col id='fde_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
