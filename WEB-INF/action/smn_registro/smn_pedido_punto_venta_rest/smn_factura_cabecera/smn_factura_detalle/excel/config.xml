<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnfacturadetalle.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_detail_bill}"/>
	
	<excel 
		recordset='query-smnfacturadetalle.sql' 
		sheetname='${lbl:b_detail_bill}' 
		filename='${lbl:b_detail_bill}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_factura_cabecera_id' label='${lbl:b_header_bill}' />
		<col id='fde_naturaleza' label='${lbl:b_nature}' />
		<col id='fde_cantidad' label='${lbl:b_quantity}' />
		<col id='fde_precio_ml' label='${lbl:b_price}' />
		<col id='fde_monto_ml' label='${lbl:b_amount}' />
		<col id='fde_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
