<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_order_health}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_pedido_cabecera_id.sql' source='sql' scope='transaction'/><recordset id='query-smn_forma_pago_rf.sql' source='sql' scope='transaction'/>
		<recordset id="smn_detalle_id" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_pedido_cabecera_id.sql' tag='smn_pedido_cabecera_id_rows'/><print mode='table' recordset='query-smn_forma_pago_rf.sql' tag='smn_forma_pago_rf_rows'/>
		<print mode="form" recordset="smn_detalle_id"/>
	</output>

</config>
