	<parameter id='smn_descuentos_retenciones_rf' type='integer' required='false' label='${lbl:b_discount_retention}' maxlen='10'/>
	<parameter id='fdr_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='fdr_fecha_registro_ini' date2='fdr_fecha_registro_fin' id='fdr_fecha_registro_ini' />
	<parameter id='fdr_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
