<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_pedido_detalle_id_name' type='varchar' required='false' label='${lbl:b_detail_bill}'/>
	<parameter id='smn_pedido_detalle_id' type='integer' required='false' label='${lbl:b_detail_bill}' maxlen='10'/>
	<parameter id='smn_mov_caja_detalle_id' type='integer' required='false' label='${lbl:b_movement_caj}' maxlen='10'/>
	<parameter id='smn_ingresos_rf' type='integer' required='false' label='${lbl:b_income}' maxlen='10'/>
	<parameter id='smn_grupo_componente_rf' type='integer' required='false' label='${lbl:b_component_group}' maxlen='10'/>
	<parameter id='pco_tipo_componente' type='varchar' required='false' label='${lbl:b_components_types}' maxlen='2'/>
	<parameter id='smn_componente_rf' type='integer' required='false' label='${lbl:b_component}' maxlen='10'/>
	<parameter id='pco_es_prestador_servicio' type='varchar' required='false' label='${lbl:b_is_services_prestator}' maxlen='10'/>
	<parameter id='smn_clase_auxiliar_rf' type='integer' required='false' label='${lbl:b_class_auxiliary}' maxlen='10'/>
	<parameter id='smn_auxiliar_rf' type='integer' required='false' label='${lbl:b_auxiliar}' maxlen='10'/>
	<parameter id='pco_naturaleza' type='varchar' required='false' label='${lbl:b_nature}' maxlen='2'/>
	<parameter id='smn_item_rf' type='integer' required='false' label='${lbl:b_item}' maxlen='10'/>
	<parameter id='smn_servicio_rf' type='integer' required='false' label='${lbl:b_services}' maxlen='10'/>
	<parameter id='pco_cantidad' type='integer' required='false' label='${lbl:b_quantity}' maxlen='10'/>
	<parameter id='smn_precio_ml_rf' type='double' required='false' label='${lbl:b_price}' maxlen='10'/>
	<parameter id='pco_monto_ml' type='double' required='false' label='${lbl:b_amount}' maxlen='10'/>
	<parameter id='smn_moneda_rf' type='integer' required='false' label='${lbl:b_coins}' maxlen='10'/>
	<parameter id='smn_tasa_rf' type='integer' required='false' label='${lbl:b_exchange_rate}' maxlen='10'/>
	<parameter id='smn_precio_ma' type='double' required='false' label='${lbl:b_price_ma}' maxlen='10'/>
	<parameter id='smn_monto_ma' type='double' required='false' label='${lbl:b_amount_ma}' maxlen='10'/>
	<parameter id='pco_imprime' type='varchar' required='false' label='${lbl:b_print}' maxlen='2'/>
	<parameter id='smn_grupo_titulo_impresion_rf' type='integer' required='false' label='${lbl:b_group_title_print}' maxlen='10'/>
	<parameter id='smn_tipo_gasto_rf' type='integer' required='false' label='${lbl:b_type}' maxlen='10'/>
	<parameter id='smn_sub_tipo_gasto_rf' type='integer' required='false' label='${lbl:b_sub_type}' maxlen='10'/>
	<parameter id='pco_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_pedido_detalle_id_name'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='pco_tipo_componente'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='pco_es_prestador_servicio'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='pco_naturaleza'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='pco_imprime'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='pco_estatus'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
