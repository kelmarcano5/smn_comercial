<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnplanventa.sql" source="session" scope="transaction" />
		<recordset id='query-smnmonedas.sql' source='sql' scope='transaction' />
	
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_plan_venta}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_plan_venta}">
				<col id='smn_entidades_rf' label='${lbl:b_item}' />
		<col id='smn_sucursal_rf' label='${lbl:b_subsidiary}' />
		<col id='smn_area_servicio_rf' label='${lbl:b_service_areas}' />
		<col id='smn_unidad_servicio_rf' label='${lbl:b_service_units}' />
		<col id='smn_almacen_rf' label='${lbl:b_store_code}' />
		<col id='smn_documento_id' label='${lbl:b_document}' />
		<col id='smn_servicio_rf' label='${lbl:b_services}' />
		<col id='smn_item_rf' label='${lbl:b_item}' />
		<col id='plv_semestre_rf' label='${lbl:b_semestre}' />
		<col id='plv_trimestre_rf' label='${lbl:b_trimestre}' />
		<col id='plv_mes_rf' label='${lbl:b_mes}' />
		<col id='plv_semana_rf' label='${lbl:b_semana}' />
		<col id='plv_fecha' label='${lbl:b_fecha}' />
		<col id='plv_cantidad_rf' label='${lbl:b_quantity}' />
		<col id='plv_precio_rf' label='${lbl:b_price}' />
		<col id='plv_monto_rf' label='${lbl:b_amount}' />
		<col id='plv_precio_ma_rf' label='${lbl:b_coins}' />
		<col id='plv_monto_ma_rf' label='${lbl:b_amount_ma}' />
		<col id='smn_moneda_rf' label='${lbl:b_coins}' />
		<col id='smn_tasa_rf' label='${lbl:b_exchange_rate}' />
		<col id='plv_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
		<sheet recordset='query-smnmonedas.sql' sheetname=''>
		<col id='mon_nombre' label='${lbl:b_name}' />
	</sheet>


	</excel>

</config>
