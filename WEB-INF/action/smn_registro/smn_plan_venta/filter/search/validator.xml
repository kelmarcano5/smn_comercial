	<parameter id='smn_almacen_rf' type='integer' required='false' label='${lbl:b_store_code}' maxlen='10'/>
	<parameter id='smn_documento_id' type='integer' required='false' label='${lbl:b_document}' maxlen='10'/>
	<parameter id='plv_semestre_rf' type='integer' required='false' label='${lbl:b_semestre}' maxlen='1'/>
	<!-- <custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='plv_semestre_rf' rule='like' /> -->
	<parameter id='plv_trimestre_rf' type='integer' required='false' label='${lbl:b_trimestre}' maxlen='1'/>
