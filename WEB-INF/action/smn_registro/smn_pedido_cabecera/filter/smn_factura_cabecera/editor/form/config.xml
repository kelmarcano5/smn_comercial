<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_header_bill}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_documento_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_entidad_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_sucursal_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_oferta_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_caja_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_cliente_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_moneda_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_tasa_rf.sql' source='sql' scope='transaction'/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_documento_id.sql' tag='smn_documento_id_rows'/>
		<print mode='table' recordset='query-smn_entidad_rf.sql' tag='smn_entidad_rf_rows'/>
		<print mode='table' recordset='query-smn_sucursal_rf.sql' tag='smn_sucursal_rf_rows'/>
		<print mode='table' recordset='query-smn_oferta_id.sql' tag='smn_oferta_id_rows'/>
		<print mode='table' recordset='query-smn_caja_rf.sql' tag='smn_caja_rf_rows'/>
		<print mode='table' recordset='query-smn_cliente_id.sql' tag='smn_cliente_id_rows'/>
		<print mode='table' recordset='query-smn_moneda_rf.sql' tag='smn_moneda_rf_rows'/>
		s<print mode='table' recordset='query-smn_tasa_rf.sql' tag='smn_tasa_rf_rows'/>
		<print mode="form" recordset="_request" /> 
	</output>

</config>
