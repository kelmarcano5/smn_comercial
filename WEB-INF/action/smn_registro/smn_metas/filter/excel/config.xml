<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnmetas.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_goals}"/>
	
	<excel 
		recordset='query-smnmetas.sql' 
		sheetname='${lbl:b_goals}' 
		filename='${lbl:b_goals}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_tipo_documento_id_combo' label='${lbl:b_tipo_documento_factura}' />
		<col id='smn_documento_id_combo' label='${lbl:b_document}' />
		<col id='smn_usuario_rf_combo' label='${lbl:b_user}' />
		<col id='met_cantidad_diaria' label='${lbl:b_diary_quantity}' />
		<col id='met_cantidad_usuario' label='${lbl:b_user_quantity}' />
		<col id='met_estatus_combo' label='${lbl:b_status}' />
		<col id='met_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
