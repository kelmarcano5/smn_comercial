<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnmetas.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_goals}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_goals}">
				<col id='smn_tipo_documento_id_combo' label='${lbl:b_tipo_documento_factura}' />
		<col id='smn_documento_id_combo' label='${lbl:b_document}' />
		<col id='smn_clase_auxiliar_rf_combo' label='${lbl:b_class_auxiliary}' />
		<col id='smn_auxiliar_rf_combo' label='${lbl:b_auxiliar}' />
		<col id='smn_rol_id_combo' label='${lbl:b_rol}' />
		<col id='smn_usuario_rf_combo' label='${lbl:b_user}' />
		<col id='met_fecha_desde' label='${lbl:b_from_date}' />
		<col id='met_fecha_hasta' label='${lbl:b_to_date}' />
		<col id='met_dias_habiles' label='${lbl:b_free_day}' />
		<col id='met_fecha_estadistica' label='${lbl:b_estadistic_date}' />
		<col id='met_cantidad_diaria' label='${lbl:b_diary_quantity}' />
		<col id='met_cantidad_usuario' label='${lbl:b_user_quantity}' />
		<col id='met_estatus_combo' label='${lbl:b_status}' />
		<col id='met_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
