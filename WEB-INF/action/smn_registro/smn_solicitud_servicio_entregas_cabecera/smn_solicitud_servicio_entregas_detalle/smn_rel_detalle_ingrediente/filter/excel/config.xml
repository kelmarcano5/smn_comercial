<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnreldetalleingrediente.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="1" cellStep="0" name="${lbl:b_relationship_ingredients_detail}"/>
	
	<excel 
		recordset='query-smnreldetalleingrediente.sql' 
		sheetname='${lbl:b_relationship_ingredients_detail}' 
		filename='${lbl:b_relationship_ingredients_detail}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_solicitud_servicio_entregas_detalle_id' label='${lbl:b_detail_services_delivery_request}' />
		<col id='smn_ingredientes_id' label='${lbl:b_ingredients}' />

	</excel>

</config>
