<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnsolicitudservicioentregasdetalle.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_detail_services_delivery_request}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_detail_services_delivery_request}">
				<col id='sec_origen_pl0' label='${lbl:b_header_services_delivery_request}' />
		<col id='smn_catalogo_item_comercio_id' label='${lbl:b_catalog_item_commerce}' />
		<col id='smn_unidad_medida_rf' label='${lbl:b_measurement_unit}' />
		<col id='sed_cantidad' label='${lbl:b_quantity}' />
		<col id='sed_requirimientos' label='${lbl:b_requeriment}' />
		<col id='sed_precio_ml' label='${lbl:b_price_ml}' />
		<col id='smn_moneda_rf' label='${lbl:b_coins}' />
		<col id='sed_precio_ma' label='${lbl:b_price_ma}' />
		<col id='sed_monto_moneda_local' label='${lbl:b_amount_ml}' />
		<col id='sed_monto_moneda_alterna' label='${lbl:b_amount_ma}' />
		<col id='sed_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
