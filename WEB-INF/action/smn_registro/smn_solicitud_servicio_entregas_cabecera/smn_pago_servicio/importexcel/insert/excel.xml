<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_solicitud_servicio_entregas_cabecera_id' type='integer' required='true' maxlen='10'/>
	<parameter id='smn_forma_pago_rf' type='integer' required='true' maxlen='10'/>
	<parameter id='smn_franquicia_rf' type='integer' required='true' maxlen='10'/>
	<parameter id='pas_numero_tarjeta' type='integer' required='true' maxlen='10'/>
	<parameter id='pas_codigo_seguridad' type='integer' required='true' maxlen='10'/>
	<parameter id='pas_documento_identidad' type='integer' required='true' maxlen='10'/>
	<parameter id='pas_nombre_pagador' type='varchar' required='true' maxlen='100'/>
	<parameter id='pas_numero_autorizacion' type='integer' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='pas_codigo_seguridad'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='pas_nombre_pagador'/>


</validator>
