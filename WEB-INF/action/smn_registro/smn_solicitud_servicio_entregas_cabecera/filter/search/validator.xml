<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='sec_origen' type='varchar' required='false' label='${lbl:b_origen}' maxlen='1'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='sec_origen' rule='like' />
	<parameter id='smn_documento_id' type='integer' required='false' label='${lbl:b_document}' maxlen='10'/>
	<parameter id='smn_cliente_rf' type='integer' required='false' label='${lbl:b_customer}' maxlen='10'/>
	<parameter id='smn_tipo_servicio_rf' type='integer' required='false' label='${lbl:b_type_services}' maxlen='10'/>
	<parameter id='sec_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='sec_fecha_registro_ini' date2='sec_fecha_registro_fin' id='sec_fecha_registro_ini' />
	<parameter id='sec_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
