<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelpedidofechaentrega.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_delivery_date_order}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_delivery_date_order}">
				<col id='smn_pedido_detalle_id' label='${lbl:b_header_order_detail}' />
		<col id='smn_rel_auxiliar_direccion' label='${lbl:b_delivery_address}' />
		<col id='pfe_fecha_entrega' label='${lbl:b_delivery_date}' />
		<col id='pfe_cantidad' label='${lbl:b_quantity}' />
		<col id='pfe_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
