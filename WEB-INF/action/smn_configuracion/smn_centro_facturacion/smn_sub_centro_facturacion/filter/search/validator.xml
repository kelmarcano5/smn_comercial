<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_centro_facturacion_id' type='integer' required='false' label='${lbl:b_centro_facturacion}' maxlen='10'/>
	<parameter id='scf_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='20'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='scf_codigo' rule='like' />
	<parameter id='scf_nombre' type='varchar' required='false' label='${lbl:b_description}' maxlen='200'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='scf_nombre' rule='like' />
	<parameter id='scf_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='scf_estatus' rule='like' />
	<parameter id='scf_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='scf_fecha_registro_ini' date2='scf_fecha_registro_fin' id='scf_fecha_registro_ini' />
	<parameter id='scf_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
