<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='smn_centro_facturacion_id' type='integer' required='true' label='${lbl:b_centro_facturacion}' maxlen='10'/>
	<parameter id='scf_codigo' type='varchar' required='true' label='${lbl:b_code}' maxlen='20'/>
	<parameter id='scf_nombre' type='varchar' required='true' label='${lbl:b_description}' maxlen='200'/>
	<parameter id='scf_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
	<parameter id='scf_vigencia' type='date' required='false' label='${lbl:b_validation_date}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='scf_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='scf_nombre'/>


</validator>
