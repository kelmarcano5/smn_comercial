<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_tipo_documentos_fact_id_name' type='varchar' required='false' label='${lbl:b_tipo_documento_factura}'/>
	<parameter id='smn_tipo_documentos_fact_id' type='integer' required='false' label='${lbl:b_tipo_documento_factura}' maxlen='10'/>
	<parameter id='smn_documentos_generales_rf' type='integer' required='false' label='${lbl:b_general_documents}' maxlen='10'/>
	<parameter id='dcf_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='dcf_descripcion' type='varchar' required='false' label='${lbl:b_description} ' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='dcf_descripcion' rule='contains' />
	<parameter id='dcf_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_greather_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_greather_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='dcf_fecha_registro_ini' date2='dcf_fecha_registro_fin' id='dcf_fecha_registro_ini' />
	<parameter id='dcf_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
