<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_relacion_documento_titulo_impresion}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_relacion_documento_titulo_impresion}' 
		filename='${lbl:b_relacion_documento_titulo_impresion}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_documento_id' label='${lbl:b_document}' />
		<col id='smn_grupo_titulo_impresion_rf' label='${lbl:b_group_tittle_print}' />
		<col id='rdi_secuencia_impresion' label='${lbl:b_secuencia}' />
		<col id='rdi_idioma' label='${lbl:s_profile_lang}' />
		<col id='rdi_usuario' label='${lbl:b_users}' />
		<col id='rdi_fecha_registro' label='${lbl:b_store_date}' />
		<col id='rdi_hora' label='${lbl:s_loginhist_col2}' />

	</excel>

</config>
