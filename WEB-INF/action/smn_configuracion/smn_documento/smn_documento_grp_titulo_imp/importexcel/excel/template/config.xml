<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smndocumentogrptituloimp.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_relacion_documento_titulo_impresion}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_relacion_documento_titulo_impresion}">
				<col id='smn_documento_id' label='${lbl:b_document}' />
		<col id='smn_grupo_titulo_impresion_rf' label='${lbl:b_group_tittle_print}' />
		<col id='rdi_secuencia_impresion' label='${lbl:b_secuencia}' />
		<col id='rdi_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
