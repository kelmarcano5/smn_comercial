<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_detalle_documento_grupo_titulo_impresion}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_detalle_documento_grupo_titulo_impresion}' 
		filename='${lbl:b_detalle_documento_grupo_titulo_impresion}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_documento_grp_titulo_imp_id' label='${lbl:b_relacion_documento_titulo_impresion}' />
		<col id='dgi_naturaleza' label='${lbl:b_document}' />
		<col id='smn_id_rf' label='${lbl:b_servicio_item}' />
		<col id='smn_componentes_rf' label='${lbl:b_components}' />
		<col id='dgi_idioma' label='${lbl:s_profile_lang}' />
		<col id='dgi_usuario' label='${lbl:b_users}' />
		<col id='dgi_fecha_registro' label='${lbl:b_store_date}' />
		<col id='dgi_hora' label='${lbl:s_loginhist_col2}' />
	</excel>

</config>
