<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_tipo_documentos_fact_id' type='integer' required='false' maxlen='10'/>
	<parameter id='smn_documentos_generales_rf' type='integer' required='true' maxlen='10'/>
	<parameter id='dcf_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='dcf_descripcion' type='varchar' required='true' maxlen='48'/>
	<parameter id='dcf_tipo_movimiento' type='varchar' required='true' maxlen='2'/>
	<parameter id='dcf_req_vigencia' type='varchar' required='true' maxlen='2'/>
	<parameter id='dcf_req_fecha_vencimiento' type='varchar' required='true' maxlen='2'/>
	<parameter id='dcf_gen_orden_despacho' type='varchar' required='true' maxlen='2'/>
	<parameter id='dcf_req_servicios' type='varchar' required='true' maxlen='2'/>
	<parameter id='dcf_req_vendedor' type='varchar' required='true' maxlen='2'/>
	<parameter id='dcf_req_tiempo_entrega' type='varchar' required='true' maxlen='2'/>
	<parameter id='dcf_usa_bonificaciones' type='varchar' required='true' maxlen='2'/>
	<parameter id='dcf_usa_descuentos' type='varchar' required='true' maxlen='2'/>
	<parameter id='dcf_moneda_alterna' type='varchar' required='false' maxlen='2'/>
	<parameter id='dcf_selecciona_precio' type='varchar' required='true' maxlen='2'/>
	<parameter id='dcf_usa_anexo' type='varchar' required='false' maxlen='2'/>
	<parameter id='dcf_vigencia' type='date' required='true' maxlen='10'/>
	<parameter id='dcf_estatus' type='varchar' required='true' maxlen='2'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='dcf_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='dcf_descripcion'/>


</validator>
