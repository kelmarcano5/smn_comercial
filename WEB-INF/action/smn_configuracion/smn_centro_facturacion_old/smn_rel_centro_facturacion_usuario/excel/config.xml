<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelcentrofacturacionusuario.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_rel_centro_facturacion_usuario}"/>
	
	<excel 
		recordset='query-smnrelcentrofacturacionusuario.sql' 
		sheetname='${lbl:b_rel_centro_facturacion_usuario}' 
		filename='${lbl:b_rel_centro_facturacion_usuario}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_centro_facturacion_id' label='${lbl:b_centro_facturacion}' />
		<col id='smn_usuario_rf' label='${lbl:b_users}' />
		<col id='rfu_vigencia' label='${lbl:b_validation_date}' />
		<col id='rfu_estatus' label='${lbl:b_status}' />
		<col id='rfu_idioma' label='${lbl:s_profile_lang}' />
		<col id='rfu_usuario' label='${lbl:b_users}' />
		<col id='rfu_fecha_registro' label='${lbl:b_store_date}' />
		<col id='rfu_hora' label='${lbl:s_loginhist_col2}' />

	</excel>

</config>
