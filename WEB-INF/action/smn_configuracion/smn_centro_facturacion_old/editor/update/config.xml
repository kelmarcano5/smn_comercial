<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		Actualiza valores de un formulario en una tabla
	</summary>

	<log>false</log>

	<!--soporte de auditoria-->
	<audit>
		<!--de donde saca los datos, por defecto es "_request"-->
		<recordset></recordset>
		<!--que campos extrae para el registro descriptivo-->
		<cols>
			<colname>cfc_codigo</colname>
			<colname>cfc_nombre</colname>
			<colname>smn_entidades_rf</colname>
			<colname>smn_sucursales_rf</colname>
			<colname>smn_areas_servicios_rf</colname>
			<colname>smn_unidades_servicios_rf</colname>
			<colname>cfc_numero_identificacion</colname>
			<colname>smn_almacen_rf</colname>
			<colname>smn_centro_costo_rf</colname>
			<colname>smn_caja_rf</colname>
			<colname>cfc_control_fiscal_1</colname>
			<colname>cfc_control_fiscal_2</colname>
			<colname>cfc_vigencia</colname>
			<colname>cfc_estatus</colname>
			<colname>smn_baremos_rf</colname>
		
		</cols>
		<table>smn_comercial.smn_centro_facturacion</table>
		<oper>ACTUALIZAR</oper>
		<area>Carga de datos</area>
		<!--
			query para obtener el ID generado o nombre de 
			la columna que contiene el pkey, si es query, 
			el alias del campo debe ser "id"
		-->
		<pkey>id</pkey>
	</audit>

	<transaction>
		<classname>dinamica.audit.AuditableTransaction</classname>
		<validator>true</validator>
		<transaction>true</transaction>
		<jdbc-log>false</jdbc-log>
	</transaction>

	<query>update.sql</query>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>script.js</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/javascript; charset=iso-8859-1</content-type>
		<expiration>0</expiration>
	</output>

</config>
