<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='gpv_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='18'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='gpv_codigo' rule='like' />
	<parameter id='gpv_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='200'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='gpv_descripcion' rule='like' />
	<parameter id='gpv_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='gpv_fecha_registro_ini' date2='gpv_fecha_registro_fin' id='gpv_fecha_registro_ini' />
	<parameter id='gpv_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
