<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='gpv_codigo' type='varchar' required='true' maxlen='18'/>
	<parameter id='gpv_descripcion' type='varchar' required='true' maxlen='200'/>
	<parameter id='gpv_estatus' type='varchar' required='true' maxlen='2'/>
	<parameter id='gpv_vigencia' type='date' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='gpv_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='gpv_descripcion'/>



</validator>
