<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_clase_auxiliar_rf' type='integer' required='true' maxlen='10'/>
	<parameter id='aux_tipo_persona' type='varchar' required='true' maxlen='2'/>
	<parameter id='aux_apellidos' type='varchar' required='false' maxlen='64'/>
	<parameter id='aux_nombres' type='varchar' required='false' maxlen='64'/>
	<parameter id='aux_tipo_doc_oficial_rf_desc_ref' type='varchar' required='false' maxlen='10'/>
	<parameter id='aux_rif' type='varchar' required='false' maxlen='16'/>

	<parameter id='aux_tipo_doc_oficial_rf_ref' type='integer' required='false' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='aux_nombres'/>


	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: aux_tipo_doc_oficial_rf' 
		parameter='aux_tipo_doc_oficial_rf_ref' 
		sql='aux_tipo_doc_oficial_rf.sql' 
		id='aux_tipo_doc_oficial_rf_ref'/>

</validator>
