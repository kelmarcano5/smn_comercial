<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='ges_codigo' type='varchar' required='true' label='${lbl:b_code}' maxlen='10'/>
	<parameter id='ges_descripcion' type='varchar' required='true' label='${lbl:b_description}' maxlen='100'/>
	<parameter id='smn_accion_id_name' type='varchar' required='true' label='${lbl:b_action}'/>
	<parameter id='smn_accion_id' type='integer' required='false' label='${lbl:b_action}' maxlen='10'/>
	<parameter id='ges_estatus' type='varchar' required='true' label='${lbl:b_status}' maxlen='2'/>
	<parameter id='ges_vigencia' type='date' required='true' label='${lbl:b_vigency}' maxlen='10'/>
	<parameter id="reference" type="varchar" required="false" label="Reference" maxlen="5" />
	
	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='ges_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='ges_descripcion'/>


</validator>
