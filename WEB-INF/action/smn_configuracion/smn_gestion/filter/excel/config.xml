<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smngestion.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_management}"/>
	
	<excel 
		recordset='query-smngestion.sql' 
		sheetname='${lbl:b_management}' 
		filename='${lbl:b_management}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='ges_codigo' label='${lbl:b_code}' />
		<col id='ges_descripcion' label='${lbl:b_description}' />
		<col id='smn_accion_id_combo' label='${lbl:b_action}' />
		<col id='ges_estatus_combo' label='${lbl:b_status}' />
		<col id='ges_vigencia' label='${lbl:b_vigency}' />
		<col id='ges_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
