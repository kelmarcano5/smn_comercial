<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='ges_codigo' type='varchar' required='true' maxlen='10'/>
	<parameter id='ges_descripcion' type='varchar' required='true' maxlen='100'/>
	<parameter id='smn_accion_id' type='integer' required='false' maxlen='10'/>
	<parameter id='ges_estatus' type='varchar' required='true' maxlen='2'/>
	<parameter id='ges_vigencia' type='date' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='ges_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='ges_descripcion'/>


</validator>
