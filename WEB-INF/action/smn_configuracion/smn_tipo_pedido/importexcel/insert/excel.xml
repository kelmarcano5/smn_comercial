<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='tpp_codigo' type='varchar' required='true' maxlen='64'/>
	<parameter id='tpp_descripcion' type='varchar' required='true' maxlen='120'/>
	<parameter id='tpp_estatus' type='varchar' required='true' maxlen='2'/>
	<parameter id='tpp_fecha_vigencia' type='date' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='tpp_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='tpp_descripcion'/>


</validator>
