<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='tpp_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='64'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tpp_codigo' rule='like' />
	<parameter id='tpp_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='120'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tpp_descripcion' rule='like' />
	<parameter id='tpp_estatus' type='varchar' required='false' label='${lbl:b_estatus}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tpp_estatus' rule='like' />
	<parameter id='tpp_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='tpp_fecha_registro_ini' date2='tpp_fecha_registro_fin' id='tpp_fecha_registro_ini' />
	<parameter id='tpp_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
