<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncontrolfiscaldocumento.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_control_fiscal_document}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_control_fiscal_document}">
				<col id='smn_documento_id' label='${lbl:b_document}' />
		<col id='smn_proveedor_rf' label='${lbl:b_proveedor}' />
		<col id='cfd_numero_documento_fiscal_inicial' label='${lbl:b_doc_num_fis_ini}' />
		<col id='cfd_numero_documento_fiscal_final' label='${lbl:b_doc_num_fis_fin}' />
		<col id='cfd_ultimo_numero_fiscal_usado' label='${lbl:b_num_ult_fis_usa}' />
		<col id='cfd_fecha_solicitud' label='${lbl:b_application_date}' />
		<col id='cfd_fecha_recepcion' label='${lbl:b_reception_date}' />
		<col id='cfd_serie' label='${lbl:b_serie}' />
		<col id='cfd_estatus' label='${lbl:b_estatus}' />
		<col id='cfd_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
