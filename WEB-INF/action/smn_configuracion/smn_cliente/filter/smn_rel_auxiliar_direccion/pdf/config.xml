<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelauxiliardireccion.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_rel_auxiliar_direccion}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_rel_auxiliar_direccion}.pdf</pdf-file-name>

	<pdf-report 
		pageSize="letter" 
		rotate="false"
		footer="false" 
		header="true">
		<record recordset='query-smnrelauxiliardireccion.sql' width='100' spacingBefore='40'>
			<col name='smn_clase_auxiliar_id_combo' title='${lbl:b_auxiliary_class}' width='12' align='left' format='######' />
			<col name='smn_auxiliar_id_combo' title='${lbl:b_auxiliary}' width='12' align='left' format='######' />
			<col name='smn_direccion_id_combo' title='${lbl:b_address}' width='12' align='left' format='######' />
			<col name='smn_zona_rf' title='${lbl:b_zone}' width='12' align='left' format='######' />
			<col name='rad_tipo_direccion' title='${lbl:b_direction_type}' width='12' align='left' />
			<col name='rad_status' title='${lbl:b_status}' width='12' align='left' />
			<col name='rad_fecha_vigencia' title='${lbl:b_fecha_vigencia}' width='12' align='left' format='dd-MM-yyyy' />
			<col name='rad_fecha_registro' title='${lbl:b_store_date}' width='12' align='left' format='dd-MM-yyyy' />

		</record>

	</pdf-report>
</config>
