<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelclienteareainteres.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_relacion_cliente_area_interes}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_relacion_cliente_area_interes}">
				<col id='smn_cliente_id' label='${lbl:b_customer}' />
		<col id='smn_area_interes_id' label='${lbl:b_areas_interes}' />
		<col id='rca_estatus' label='${lbl:b_status}' />
		<col id='rca_vigencia' label='${lbl:b_validation_date}' />
		<col id='rca_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
