<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_cliente_id' type='integer' required='false' label='${lbl:b_customer}' maxlen='10'/>
	<parameter id='smn_condicion_financiera_rf' type='integer' required='false' label='${lbl:b_condition_financiery}' maxlen='10'/>
	<parameter id='rcc_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='rcc_estatus' rule='like' />
	<parameter id='rcc_vigencia' type='date' required='false' label='${lbl:b_vigency}' maxlen='10'/>
	<parameter id='rcc_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='rcc_fecha_registro_ini' date2='rcc_fecha_registro_fin' id='rcc_fecha_registro_ini' />
	<parameter id='rcc_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
