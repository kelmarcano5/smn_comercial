<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelclienteentidadfinanciera.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_relacion_cliente_entidad_financiera}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_relacion_cliente_entidad_financiera}">
				<col id='smn_cliente_id' label='${lbl:b_customer}' />
		<col id='smn_entidad_financiera_rf' label='${lbl:b_financial_entities}' />
		<col id='smn_tipo_cuenta_banco_rf' label='${lbl:b_types_of_bank_accounts}' />
		<col id='cef_numero_cuenta' label='${lbl:b_bank_account_number}' />
		<col id='cef_estatus' label='${lbl:b_status}' />
		<col id='cef_vigencia' label='${lbl:b_validation_date}' />
		<col id='cef_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
