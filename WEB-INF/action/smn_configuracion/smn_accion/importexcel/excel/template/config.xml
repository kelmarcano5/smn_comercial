<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnaccion.sql" source="session" scope="transaction" />
		<recordset id='query-smnpaso.sql' source='sql' scope='transaction' />
	
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_action}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_action}">
				<col id='acc_codigo' label='${lbl:b_code}' />
		<col id='acc_descripcion' label='${lbl:b_description}' />
		<col id='acc_require_seguimiento_combo' label='${lbl:b_required_day}' />
		<col id='acc_dias_seguimiento' label='${lbl:b_followup_days}' />
		<col id='acc_paso_rf_combo' label='${lbl:b_pass}' />
		<col id='acc_estatus_combo' label='${lbl:b_status}' />
		<col id='acc_vigencia' label='${lbl:b_vigency}' />
		<col id='acc_idioma' label='${lbl:s_profile_lang}' />
		<col id='acc_usuario' label='${lbl:b_users}' />
		<col id='acc_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
		<sheet recordset='query-smnpaso.sql' sheetname=''>
		<col id='pas_descripcion' label='${lbl:b_description}' />
	</sheet>


	</excel>

</config>
