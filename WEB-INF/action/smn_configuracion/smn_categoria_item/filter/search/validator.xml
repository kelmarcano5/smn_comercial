<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='cai_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='cai_codigo' rule='like' />
	<parameter id='cai_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='cai_descripcion' rule='like' />
	<parameter id='cai_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='cai_estatus' rule='like' />
	<parameter id='cai_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='cai_fecha_registro_ini' date2='cai_fecha_registro_fin' id='cai_fecha_registro_ini' />
	<parameter id='cai_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
