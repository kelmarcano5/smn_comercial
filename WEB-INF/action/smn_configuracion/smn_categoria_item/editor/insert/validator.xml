<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='cai_codigo' type='varchar' required='true' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='cai_descripcion' type='varchar' required='true' label='${lbl:b_description}' maxlen='100'/>
	<parameter id='cai_estatus' type='varchar' required='true' label='${lbl:b_status}' maxlen='2'/>
	<parameter id='cai_fecha_vigencia' type='date' required='false' label='${lbl:b_vigency}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='cai_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='cai_descripcion'/>


</validator>
