<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncategoriaitem.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_item_category}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_item_category}">
				<col id='cai_codigo' label='${lbl:b_code}' />
		<col id='cai_descripcion' label='${lbl:b_description}' />
		<col id='cai_estatus_combo' label='${lbl:b_status}' />
		<col id='cai_fecha_vigencia' label='${lbl:b_vigency}' />
		<col id='cai_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
