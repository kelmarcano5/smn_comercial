<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='cic_origen' type='varchar' required='false' label='${lbl:b_origen}' maxlen='2'/>
	<parameter id='smn_cliente_rf' type='integer' required='false' label='${lbl:b_customer}' maxlen='10'/>
	<parameter id='smn_item_rf' type='integer' required='false' label='${lbl:b_item}' maxlen='10'/>
	<parameter id='smn_clasificacion_abc_rf_a' type='integer' required='false' label='${lbl:b_clasification_abc_a}' maxlen='10'/>
	<parameter id='smn_clasificacion_abc_rf_b' type='integer' required='false' label='${lbl:b_clasification_abc_b}' maxlen='10'/>
	<parameter id='smn_clasificacion_abc_rf_c' type='integer' required='false' label='${lbl:b_clasification_abc_c}' maxlen='10'/>
	<parameter id='smn_baremos_rf' type='integer' required='false' label='${lbl:b_baremo}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='cic_origen'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
