<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_solicitud_servicio_entregas_detalle_id_name' type='varchar' required='false' label='${lbl:b_detail_services_delivery_request}'/>
	<parameter id='smn_solicitud_servicio_entregas_detalle_id' type='integer' required='false' label='${lbl:b_detail_services_delivery_request}' maxlen='10'/>
	<parameter id='smn_item_rf' type='integer' required='false' label='${lbl:b_item}' maxlen='10'/>
	<parameter id='smn_unidad_medida_rf' type='integer' required='false' label='${lbl:b_measurement_unit}' maxlen='10'/>
	<parameter id='igs_cantidad' type='integer' required='false' label='${lbl:b_quantity}' maxlen='10'/>
	<parameter id='igs_precio' type='double' required='false' label='${lbl:b_price}' maxlen='10'/>
	<parameter id='smn_moneda_rf' type='integer' required='false' label='${lbl:b_coins}' maxlen='10'/>
	<parameter id='igs_precio_ma' type='double' required='false' label='${lbl:b_price_ma}' maxlen='10'/>
	<parameter id='igs_monto_moneda_local' type='double' required='false' label='${lbl:b_amount_ml}' maxlen='10'/>
	<parameter id='igs_monto_moneda_alterna' type='double' required='false' label='${lbl:b_amount_ma}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_solicitud_servicio_entregas_detalle_id_name'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
