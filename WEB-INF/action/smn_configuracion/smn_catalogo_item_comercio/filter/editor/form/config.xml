<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_catalog_item_commerce}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_cliente_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_item_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_categoria_item_id_a.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_categoria_item_id_b.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_categoria_menu_id.sql' source='sql' scope='transaction'/>  
		<recordset id='query-smn_baremos_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_unidad_medida_rf.sql' source='sql' scope='transaction'/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_cliente_rf.sql' tag='smn_cliente_rf_rows'/>
		<print mode='table' recordset='query-smn_item_rf.sql' tag='smn_item_rf_rows'/>
		<print mode='table' recordset='query-smn_baremos_rf.sql' tag='smn_baremos_rf_rows'/>
		<print mode='table' recordset='query-smn_unidad_medida_rf.sql' tag='smn_unidad_medida_rf_rows'/>
		<print mode='table' recordset='query-smn_categoria_item_id_a.sql' tag='smn_categoria_item_id_a_rows'/>
		<print mode='table' recordset='query-smn_categoria_item_id_b.sql' tag='smn_categoria_item_id_b_rows'/>
		<print mode='table' recordset='query-smn_categoria_menu_id.sql' tag='smn_categoria_menu_id_rows'/>
		<print mode="form" recordset="_request" /> 
	</output>

</config>
