<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smningredientes.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_ingredients}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_ingredients}">
				<col id='smn_solicitud_servicio_entregas_detalle_id' label='${lbl:b_detail_services_delivery_request}' />
		<col id='smn_item_rf' label='${lbl:b_item}' />
		<col id='smn_unidad_medida_rf' label='${lbl:b_measurement_unit}' />
		<col id='igs_cantidad' label='${lbl:b_quantity}' />
		<col id='igs_precio' label='${lbl:b_price}' />
		<col id='smn_moneda_rf' label='${lbl:b_coins}' />
		<col id='igs_precio_ma' label='${lbl:b_price_ma}' />
		<col id='igs_monto_moneda_local' label='${lbl:b_amount_ml}' />
		<col id='igs_monto_moneda_alterna' label='${lbl:b_amount_ma}' />
		<col id='igs_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
