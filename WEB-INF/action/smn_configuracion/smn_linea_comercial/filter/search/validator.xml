<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='lco_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='lco_nombre' type='varchar' required='false' label='${lbl:b_name}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='lco_nombre' rule='contains' />
	<parameter id='lco_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_greather_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_greather_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='lco_fecha_registro_ini' date2='lco_fecha_registro_fin' id='lco_fecha_registro_ini' />
	<parameter id='lco_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
