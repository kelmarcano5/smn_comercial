<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_tipo_linea_comercial_id' type='integer' required='false' maxlen='10'/>
	<parameter id='lco_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='lco_nombre' type='varchar' required='true' maxlen='48'/>
	<parameter id='lco_vigencia' type='date' required='true' maxlen='10'/>
	<parameter id='lco_estatus' type='varchar' required='true' maxlen='2'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='lco_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='lco_nombre'/>


</validator>
