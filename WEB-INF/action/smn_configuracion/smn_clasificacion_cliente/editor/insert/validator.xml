<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='ccl_codigo' type='varchar' required='true' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='ccl_descripcion' type='varchar' required='true' label='${lbl:b_description}' maxlen='48'/>
	<parameter id='ccl_vigencia' type='date' required='true' label='${lbl:b_validation_date}' maxlen='10'/>
	<parameter id='ccl_estatus' type='varchar' required='true' label='${lbl:b_status}' maxlen='2'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='ccl_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='ccl_descripcion'/>


</validator>
