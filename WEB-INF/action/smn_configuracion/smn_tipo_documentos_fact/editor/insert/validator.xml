<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='tdf_codigo' type='varchar' required='true' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='tdf_nombre' type='varchar' required='true' label='${lbl:b_name}' maxlen='48'/>
	<parameter id='tdf_naturaleza' type='varchar' required='true' label='${lbl:b_naturaleza}' maxlen='2'/>
	<parameter id='tdf_vigencia' type='date' required='true' label='${lbl:b_validation_date}' maxlen='10'/>
	<parameter id='tdf_estatus' type='varchar' required='true' label='${lbl:b_status}' maxlen='2'/>
	<parameter id="reference" type="varchar" required="false" label="Reference" maxlen="5" />

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='tdf_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='tdf_nombre'/>



</validator>
