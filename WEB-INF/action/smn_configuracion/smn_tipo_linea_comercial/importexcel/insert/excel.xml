<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='tlc_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='tlc_nombre' type='varchar' required='true' maxlen='48'/>
	<parameter id='tlc_naturaleza' type='varchar' required='true' maxlen='2'/>
	<parameter id='tlc_vigencia' type='date' required='true' maxlen='10'/>
	<parameter id='tlc_estatus' type='varchar' required='true' maxlen='2'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='tlc_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='tlc_nombre'/>


</validator>
