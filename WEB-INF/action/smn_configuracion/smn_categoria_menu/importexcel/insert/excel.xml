<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='cam_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='cam_descripcion' type='varchar' required='true' maxlen='100'/>
	<parameter id='cam_estatus' type='varchar' required='true' maxlen='2'/>
	<parameter id='cam_fecha_vigencia' type='date' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='cam_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='cam_descripcion'/>


</validator>
