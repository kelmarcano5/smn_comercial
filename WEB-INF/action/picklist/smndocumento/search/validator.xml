	<parameter id='dcf_tipo_movimiento' type='varchar' required='false' label='${lbl:b_accounting_movement_type}' maxlen='2'/>
	<parameter id='dcf_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
